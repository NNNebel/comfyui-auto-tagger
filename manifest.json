{
    "id": "f4831c52-fee6-40b8-a48a-e4ec3a4c128a",
    "version": "1.2.0",
    "platform": "all",
    "arch": "all",
    "fallbackLanguage": "ja_JP",
    "languages": ["en", "ja_JP"],
    "name": "{{manifest.name}}",
    "default_locale": "en",
    "logo": "logo.png",
    "keywords": ["comfyui", "webp", "tag", "exif", "metadata"],
    "devTools": false,
    "type": "window",
    "main": {
        "url": "index.html",
        "width": 640,
        "height": 480
    },
    "permissions": [
        "filesystem",
        "network"
    ],
    "app": {
        "desktop": {
            "contextMenu": [
                {
                    "label": "{{manifest.contextMenu_label}}",
                    "target": "image",
                    "key": "comfyui-auto-tagger.open"
                }
            ]
        }
    }
}
